# Statements #
##################################################################################

SET Citation = {"PubMed", "Front Med. 2020 Mar 12. doi: 10.1007/s11684-020-0754-0", "32170560"}
SET PublicationType = "Research"
SET PublicationStatus = "Published"
SET Section = "Results"
SET Custom_Disease = "COVID-19"
SET Species = "9606"

SET Evidence = "Interestingly, the respiratory epithelial cells from the respiratory \
track sample contained approximately 2% ACE2 positive cells, and the \
respiratory track is thus regarded as high risk (Fig. 1)."

SET Anatomy = {"respiratory tract"}
complex(p(HGNC:ACE2),a(TAX:"Severe acute respiratory syndrome coronavirus 2")) -> bp(GO:"viral entry into host cell")
bp(GO:"viral entry into host cell")->  path(MESH:Virulence)
bp(GO:"viral entry into host cell") -> path(EFO:pathogenicity)
UNSET Anatomy

SET Evidence = "The scRNA-seq data from human heart showed that more than 7.5% \
myocardial cells have positive ACE2 expression (Fig. 2), thereby \
implying that heart could be in high risk of 2019-nCoV infection, \
especially in the presence of the virus in blood or viremia."

SET Anatomy = {"myocardium", "heart"}
complex(p(HGNC:ACE2),a(TAX:"Severe acute respiratory syndrome coronavirus 2")) -> bp(GO:"viral entry into host cell")
bp(GO:"viral entry into host cell")->  path(MESH:Virulence)
bp(GO:"viral entry into host cell") -> path(EFO:pathogenicity)
UNSET Anatomy

SET Evidence = "Interestingly, extremely high ACE2 expression was found in ileal \
epithelial cells (~30% ACE2-positive cells, Fig. 3); thus, ileum could \
be at high risk."

SET Anatomy = {"ilium"}
complex(p(HGNC:ACE2),a(TAX:"Severe acute respiratory syndrome coronavirus 2")) -> bp(GO:"viral entry into host cell")
bp(GO:"viral entry into host cell")->  path(MESH:Virulence)
bp(GO:"viral entry into host cell") -> path(EFO:pathogenicity)
UNSET Anatomy

SET Evidence = "More than 1% ACE2 positive esophagus epithelial cells were found, and \
esophagus can thus be regarded as high risk (Fig. 4)."

SET MeSHAnatomy = {"Esophagus"}
complex(p(HGNC:ACE2),a(TAX:"Severe acute respiratory syndrome coronavirus 2")) -> bp(GO:"viral entry into host cell")
bp(GO:"viral entry into host cell")->  path(MESH:Virulence)
bp(GO:"viral entry into host cell") -> path(EFO:pathogenicity)
UNSET MeSHAnatomy

SET Evidence = "Significantly high ACE2 expression was found in kidney proximal tubule \
(PT) cells with the proportion of ACE2-positive cells at approximately \
4% (Fig. 5). Furthermore, the proportion of ACE2-positive cells in the \
bladder urothelial cells was 2.4% (Fig. 6).Therefore, kidney and \
bladder were considered high risk, especially in viremia."

SET MeSHAnatomy = {"Kidney Tubules, Proximal"}
SET Anatomy = {"bladder organ"}
complex(p(HGNC:ACE2),a(TAX:"Severe acute respiratory syndrome coronavirus 2")) -> bp(GO:"viral entry into host cell")
bp(GO:"viral entry into host cell")->  path(MESH:Virulence)
bp(GO:"viral entry into host cell") -> path(EFO:pathogenicity)
UNSET Anatomy
UNSET MeSHAnatomy
UNSET Species
UNSET Custom_Disease
UNSET PublicationType
UNSET PublicationStatus
UNSET Section

