# Statements #
##################################################################################

SET Citation = {"PubMed", "Crit Care. 2020 Mar 16;24(1):91. doi: 10.1186/s13054-020-2818-6.", "32178711"}
SET PublicationType = "Editorial"
SET PublicationStatus = "Published"
SET Section = "Full Text"
SET Custom_Disease = "COVID-19"
SET Evidence = "The mortality appears to be around 2%; early published data indicate 25.9% with SARS-CoV-2 pneumonia required ICU admission and 20.1% developed acute respiratory distress syndrome"

path(DO:"COVID-19") -- path(EFO:"acute respiratory distress syndrome")

SET Evidence = "The management of patients mainly focuses on the provision of supportive care, e.g., oxygenation, ventilation, and fluid management."

bp(MESH:"Oxygen Inhalation Therapy") -| path(DO:"COVID-19")
bp(MESH:Ventilation) -| path(DO:"COVID-19")
bp(MESH:"Fluid Therapy") -| path(DO:"COVID-19")

SET Evidence = "Combination treatment of low-dose systematic corticosteroids and anti-virals and atomization inhalation of interferon have been encouraged as part of critical COVID-19 management [2]."

composite(a(CHEBI:corticosteroid),a(CHEBI:"antiviral agent"),a(CHEBI:interferon)) -| path(DO:"COVID-19")

SET Evidence = "Other reported therapeutic agents that are used for the treatment of seriously ill patients have been noted in Table 1."

a(CHEBI:oseltamivir) -| path(DO:"COVID-19")
a(CHEBI:ganciclovir) -| path(DO:"COVID-19")
a(PUBCHEM:121304016) -| path(DO:"COVID-19")
#in chebi (CHEBI:145994) but not in arti
a(CHEBI:lopinavir) -| path(DO:"COVID-19")
a(CHEBI:ritonavir) -| path(DO:"COVID-19")
a(CHEBI:chloroquine) -| path(DO:"COVID-19")
a(MESH:"Medicine, Chinese Traditional") -| path(DO:"COVID-19")

SET Evidence = "China National Biotec Group Co has claimed that 10 seriously ill patients receiving this immunoglobulin therapy demonstrated improved oxygenation and reduced inflammation and viral load [7]."

path(MESH:Convalescence) -| path(DO:"COVID-19")
path(MESH:Convalescence) -> bp(GOBP:"oxygen transport")
path(MESH:Convalescence) -| path(MESH:Inflammation)
UNSET Custom_Disease
UNSET PublicationType
UNSET PublicationStatus
UNSET Section
