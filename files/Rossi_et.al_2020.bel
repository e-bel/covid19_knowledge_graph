##################################################################################
# Statements #
##################################################################################

SET Citation = {"DOI", "https://doi.org/10.1101/2020.04.13.20063545"}
SET PublicationType = "Research"
SET PublicationStatus = "Preprint"
SET Section = "Results"

SET Species = "9606"
SET Custom_Disease = "COVID-19"
SET Evidence = "After a median follow up of 14 days, 1075 (40%) and 217 (8.2%) experienced hospitalization and death, respectively. The rates of both these events were substantially higher in males (50% vs 31% for hospital admission, and 11% vs 6% for death)"

a(MESH:Male) pos path(MESH:Mortality)
a(MESH:Male) pos a(MESH:Hospitalization)

SET Evidence = " As for single comorbidities (the most prevalent being hypertension, cancer, and diabetes),all were associated with high (i.e. above 50%) rates of hospitalization and death (except obesity, above 15%)"

path(DO:hypertension) pos path(MESH:Mortality)
path(DO:cancer) pos path(MESH:Mortality)
path(DO:"diabetes mellitus") pos path(MESH:Mortality)

SET Evidence = "Immigration status (as represented by place of birth) was found to be associated with hospitalization, with patients born abroad having a 40% higher risk. "

a(MESH:"Emigration and Immigration") pos a(MESH:Hospitalization)
a(MESH:"Emigration and Immigration") pos a(MESH:"Risk Factors")

SET Evidence = "Longer time span from symptom onset to diagnosis had a lower risk of hospitalization and death, thus confirming that a shorter length of that interval indicates worse clinical condition."

a(MESH:"Delayed Diagnosis") pos a(MESH:"Risk Factors")
a(MESH:"Delayed Diagnosis") pos a(MESH:Hospitalization)

SET Evidence = "As shown in Table 5, COPD, chronic kidney disease, and heart failure had the strongest association with the risk of hospitalization, adjusting for age and sex. "

path(DO:"chronic kidney disease") pos a(MESH:Hospitalization)
path(MESH:"Heart Failure") pos a(MESH:Hospitalization)
path(DO:"chronic obstructive pulmonary disease") pos a(MESH:Hospitalization)

SET Evidence = " As for the use of AT-1 inhibitors and ACE inhibitors, exposure to these drugs appeared to be associated with a modest increase in hospitalization risk which, for ACE inhibitors, was not compatible with a random fluctuation."

a(CHEBI:"angiotensin receptor antagonist") pos a(MESH:Hospitalization)
a(MESH:"Angiotensin-Converting Enzyme Inhibitors") pos a(MESH:Hospitalization)
a(CHEBI:"angiotensin receptor antagonist") pos a(MESH:"Risk Factors")
a(MESH:"Angiotensin-Converting Enzyme Inhibitors") pos a(MESH:"Risk Factors")

SET Evidence = "The highest risk of death was seen in patients with cardiovascular comorbidities (heart failure, arrhythmia, ischemic heart disease), followed by dementia and diabetes. "

path(MESH:"Heart Failure") pos path(MESH:Mortality)
path(HP:Arrhythmia) pos path(MESH:Mortality)
path(EFO:"ischemic cardiomyopathy") pos path(MESH:Mortality) 
path(DO:dementia) pos path(MESH:Mortality)
path(DO:"diabetes mellitus") pos path(MESH:Mortality)
UNSET Custom_Disease
UNSET Species
UNSET PublicationStatus
UNSET PublicationType
UNSET Section