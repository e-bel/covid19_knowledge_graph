##################################################################################
# Statements #
##################################################################################

SET Citation = {"PubMed","32201080"}
SET PublicationType = "Research"
SET PublicationStatus = "Published"
SET Section = "Results"

SET Evidence = "Human ACE2 is the receptor for both SARS-CoV and SARS-CoV-2."

SET Species = "9606"
complex(p(HGNC:ACE2), a(TAX:"Severe acute respiratory syndrome-related coronavirus")) -> path(DO:"severe acute respiratory syndrome")
SET Custom_Disease = "COVID-19"
complex(p(HGNC:ACE2), a(TAX:"Severe acute respiratory syndrome coronavirus 2")) -> path(DO:"COVID-19")


SET Evidence = "According to the literature [15], the key amino acids (AAs) in the \
Sprotein of SARS-CoV interacting with human ACE2 are Y442, L472,N479, \
D480, T487, Y491 [7,15]."

complex(p(HGNC:ACE2), p(COVID:S)) pos bp(MESH:Virulence)
UNSET Species

SET Evidence = "Some Pets including dog(Canis lupus familiaris) and cat (Felis catus) \
potentially recognize S protein, indicating the importance to monitor \
the pets for SARS-CoV-2 infection."

complex(p(UNIPROT:J9P7Y2), p(COVID:S)) pos bp(MESH:Virulence)
#480847 - ACE2 from Canis lupus familiaris, not in arti file
complex(p(UNIPROT:Q56H28), p(COVID:S)) pos bp(MESH:Virulence)
#554349 ACE2 from Felis catus,  not in arti file

SET Evidence = "We noticed that ACE2 from dog, cat, pangolin and Chinese hamster \
potentially associated with S protein (Table 1)."

complex(p(NCBIGENE:108390919), p(COVID:S)) pos bp(MESH:Virulence)
#108390919 - ACE2 from pangolin,  not in arti file
complex(p(NCBIGENE:100772552), p(COVID:S)) pos bp(MESH:Virulence)
#100772552 -Ace2 from Chinese hamster,  not in arti file
UNSET Custom_Disease
UNSET Section
UNSET PublicationStatus
UNSET PublicationType