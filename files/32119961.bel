##############
# Statements #
##############
SET Citation = {"DOI","https://doi.org/10.1016/j.lfs.2020.117477"}

SET Evidence = "Until today, six different strains of Human coronaviruses (HCoVs) have been reported, in addition to the newly emerged COVID-19 [2,12]. 229E and NL63 strains of HCoVs belong to Alphacoronaviruses while OC43, HKU1, SARS, MERS, and COVID-19 HCoVs belong to Betacoronaviruses [2,11]."
a(MESH:"Coronavirus 229E, Human") isA a(NCBITAXON:Alphacoronavirus)
a(MESH:"Coronavirus NL63, Human") isA a(NCBITAXON:Alphacoronavirus)
a(MESH:"Coronavirus OC43, Human") isA a(NCBITAXON:Betacoronavirus)
a(NCBITAXON:"Human coronavirus HKU1") isA a(NCBITAXON:Betacoronavirus)
a(MESH:"SARS Virus") isA a(NCBITAXON:Betacoronavirus)
a(MESH:"Middle East Respiratory Syndrome Coronavirus") isA a(NCBITAXON:Betacoronavirus)
path(DO:"COVID-19") isA a(NCBITAXON:Betacoronavirus)

SET Evidence  = "Different directly acting antiviral drugs are approved against other viruses, by the Food and Drugs Administration (FDA), such as Sofosbuvir, Ribavirin against RdRp of Hepatitis C Virus (HCV)."
SET MeSHDisease = "Hepatitis, Viral, Human"
a(CHEBI:sofosbuvir) -| p(MESH:"RNA Replicase")
a(CHEBI:ribavirin) -| p(MESH:"RNA Replicase")
UNSET MeSHDisease

SET Experiment = "Molecular docking"
#all considered as inhibitor for covid-19

SET Evidence = "As reflected from the docking scores, the eight compounds, including the physiological GTP and UTP and the four drugs IDX-184, Sofosbuvir, Ribavirin, and Remidisvir can bind to both COVID-19 and SARS HCoV RdRps with good binding energy (−6.5 up to −9.0 kcal/mol)."
#135565589 - IDX-184, 121304016 - Remidisvir
complex(a(CHEBI:GTP),p(MESH:"RNA Replicase",frag("255_256"))) -| path(DO:"COVID-19")
complex(a(PUBCHEM:135565589),p(MESH:"RNA Replicase",frag("255_256"))) -| path(DO:"COVID-19")
complex(a(CHEBI:sofosbuvir),p(MESH:"RNA Replicase",frag("255_256"))) -| path(DO:"COVID-19")
complex(a(CHEBI:ribavirin),p(MESH:"RNA Replicase",frag("255_256"))) -| path(DO:"COVID-19")
complex(a(PUBCHEM:121304016),p(MESH:"RNA Replicase",frag("255_256"))) -| path(DO:"COVID-19")

SET Evidence = "Only one salt bridge (yellow spheres connected by a dashed line) is formed in the case of IDX-184 with D514, which is responsible for the increased stabilization of the formed complex, reflected on the docking score."
#135565589 - IDX-184
complex(a(PUBCHEM:135565589),p(MESH:"RNA Replicase",frag("514_514"))) -| path(DO:"COVID-19")

SET Evidence = "On the other hand, IDX-184 and Sofosbuvir both form metal interaction through the Mg+2 with D652 (two interactions) and E702, respectively."
#135565589 - IDX-184
complex(a(PUBCHEM:135565589),p(MESH:"RNA Replicase",frag("652_652")),a(CHEBI:"magnesium(2+)")) -| path(DO:"COVID-19")
complex(a(CHEBI:sofosbuvir),p(MESH:"RNA Replicase",frag("652_652")),a(CHEBI:"magnesium(2+)")) -| path(DO:"COVID-19")
complex(a(PUBCHEM:135565589),p(MESH:"RNA Replicase",frag("702_702")),a(CHEBI:"magnesium(2+)")) -| path(DO:"COVID-19")
complex(a(CHEBI:sofosbuvir),p(MESH:"RNA Replicase",frag("702_702")),a(CHEBI:"magnesium(2+)")) -| path(DO:"COVID-19")

SET Evidence = "Additionally, Sofosbuvir formed two hydrophobic interactions with Y510 and D651 of the COVID-19 RdRp."
complex(a(CHEBI:sofosbuvir),p(MESH:"RNA Replicase",frag("510_510"))) -| path(DO:"COVID-19")
complex(a(CHEBI:sofosbuvir),p(MESH:"RNA Replicase",frag("651_651"))) -| path(DO:"COVID-19")

UNSET Experiment
