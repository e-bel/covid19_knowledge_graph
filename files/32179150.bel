##################################################################################
# Statements #
##################################################################################

SET Citation = {"PubMed","32179150"}

SET PublicationType = "Editorial"
SET PublicationStatus = "Preprint"
SET Section = "Full Text"

SET Evidence = "This coronavirus, named SARS-CoV-2 (formerly 2019-nCoV), is responsible for respiratory infections including pneumonia with a mortality rate estimated about 2%-2.5%, increasing with age and the existence of underlying diseases"
a(TAX:"Severe acute respiratory syndrome coronavirus 2") -> path(DO:"COVID-19")
path(DO:"COVID-19") pos path(DO:"viral pneumonia")
path(DO:"COVID-19") pos path(HP:"human aged stage")

SET Evidence = "Chloroquine/hydroxychloroquine, a front-line drug used in the treatment and prophylaxis of malaria also used in autoimmune disease had been shown to inhibit the replication of several DNA and RNA viruses, including most of human coronaviruses"
a(CHEBI:chloroquine) -| path(DO:malaria)
a(CHEBI:hydroxychloroquine) -| path(DO:malaria)
a(CHEBI:chloroquine) -| path(MESH:"Autoimmune Diseases")
a(CHEBI:hydroxychloroquine) -| path(MESH:"Autoimmune Diseases")
a(CHEBI:chloroquine) -| path(MESH:"Virus Replication")
a(CHEBI:hydroxychloroquine) -| path(MESH:"Virus Replication")

SET Evidence = "Recently, chloroquine was found to inhibit SARS-CoV-2 in vitro and its hydroxylated form has been proposed as a possible therapy to treat patients infected with SARS-CoV-2 [4,5]."
a(CHEBI:chloroquine) -| a(TAX:"Severe acute respiratory syndrome coronavirus 2")
a(CHEBI:hydroxychloroquine) -| a(TAX:"Severe acute respiratory syndrome coronavirus 2")

SET Evidence = "In this contest, other drug showed significant efficacy against SARS-Cov-2 in vitro such as remdesivir, lopinavir, ribavirin or ritonavir (https://drugvirus.info/) [6]."
a(PUBCHEM:121304016) -| a(TAX:"Severe acute respiratory syndrome coronavirus 2")
a(CHEBI:lopinavir) -| a(TAX:"Severe acute respiratory syndrome coronavirus 2")
a(CHEBI:ribavirin) -| a(TAX:"Severe acute respiratory syndrome coronavirus 2")
a(CHEBI:ritonavir) -| a(TAX:"Severe acute respiratory syndrome coronavirus 2")

SET Evidence = "Teicoplanin, a glycopeptide antibiotic routinely used to treat bacterial infection was found to be active in vitro against SARS-CoV, has joined the list of molecules that could be used as therapeutic arsenal against COVID-19 [7]."
a(CHEBI:teicoplanin) -| a(TAX:"Severe acute respiratory syndrome coronavirus 2")

SET Evidence = "This antibiotic, currently used in the treatment of Gram-positive bacterial infection, especially in Staphylococcal infections, has already showed efficacy against various viruses such as Ebola, influenza virus, flavivirus, hepatitis C virus, HIV virus and on coronavirus such as MERS-CoV and SARS-CoV [8,9]."
a(CHEBI:teicoplanin) -| path(MESH:"Staphylococcal Infections")
a(CHEBI:teicoplanin) -| a(TAX:"Ebola virus")
a(CHEBI:teicoplanin) -| path(DO:influenza)
a(CHEBI:teicoplanin) -| a(TAX:Flavivirus)
a(CHEBI:teicoplanin) -| a(TAX:Hepacivirus)
a(CHEBI:teicoplanin) -| a(TAX:"Human immunodeficiency virus")
a(CHEBI:teicoplanin) -| a(TAX:"Severe acute respiratory syndrome-related coronavirus")
a(CHEBI:teicoplanin) -| a(TAX:"Middle East respiratory syndrome-related coronavirus")

SET Evidence = "According to Zhou and colleagues, in coronaviruses, teicoplanin acts on the early step of the viral life cycle by inhibiting the low pH cleavage of the viral spike protein by cathepsin L in the late endosomes thereby preventing the release of genomic viral RNA and the continuation of virus replication cycle."
SET Species = "9606"
a(CHEBI:teicoplanin) -| act(p(HGNC:CTSL))
act(p(HGNC:CTSL)) reg a(MESH:"Spike Glycoprotein, Coronavirus")
a(MESH:"Spike Glycoprotein, Coronavirus") reg bp(GO:"viral entry into host cell")
a(MESH:"Spike Glycoprotein, Coronavirus") reg bp(GO:"viral life cycle")

SET Evidence = "A recent study by the same authors showed that this activity was conserved on SARSCov- 2 (the target sequence that serve as cleavage site for cathepsin L is conserved among SARS-CoV spike protein) [7]."
act(p(HGNC:CTSL)) -> p(COVID:S, frag("?"))
p(COVID:S, frag("?")) -> bp(GO:"viral entry into host cell")
p(COVID:S, frag("?")) -> bp(GO:"viral life cycle")
UNSET Species
UNSET PublicationType
UNSET PublicationStatus
UNSET Section

