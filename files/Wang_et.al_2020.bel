# Statements #
##################################################################################
#example#
SET Citation = {"DOI","https://doi.org/10.1101/2020.03.14.988345"}
SET PublicationType = "Research"
SET PublicationStatus = "Preprint"
SET Section = "Results"


SET Evidence = "These results show that blocking CD147 on the host cells has an \
inhibitory effect on SARS-CoV-2, suggesting an essential role of CD147 \
in facilitating SARS-CoV-2 invasion for host cells."

SET CellLine = "Vero cell"
SET CellLine_Infection_Type = "Severe acute respiratory syndrome coronavirus 2"
act(p(UNIPROT:A0A0D9RKP4)) -> bp(GO:"viral entry into host cell")

SET Evidence = "Surface Plasmon Resonance (SPR) assay confirmed the interaction of \
CD147 and SP (RBD), with an affinity constant of 1.85×10-7M (Figure \
2A)."

SET BioAssay = "surface plasmon resonance"
complex(p(COVID:S), p(UNIPROT:A0A0D9RKP4))
UNSET BioAssay

SET Evidence = "The result showed that there was an interaction between CD147 and SP \
(RBD), which was consistent with the SPR assay (Figure 2B)."

SET Assay = "Co-Immunoprecipitation"
complex(p(COVID:S), p(UNIPROT:A0A0D9RKP4))
UNSET Assay

SET Evidence = "In addition, the interaction of CD147-SP (RBD) was also confirmed by \
ELISA assay (Figure 2C)."

SET BioAssay = "ELISA"
complex(p(COVID:S), p(UNIPROT:A0A0D9RKP4))

SET Evidence = "In the meanwhile, competitive inhibition assay showed that Meplazumab \
could competitively inhibit the binding of SP and CD147, with an IC50 \
of 16.44 μg/mL (Figure 2D)."

a(DRUGBANK:DB06612) -| complex(p(COVID:S), p(UNIPROT:A0A0D9RKP4))
UNSET BioAssay

SET Evidence = "These results confirm the interaction of CD147 and SP, indicating a \
novel potential route for SARS-CoV-2 to invade host cells."

complex(p(COVID:S), p(UNIPROT:A0A0D9RKP4)) -> bp(GO:"viral entry into host cell")

UNSET CellLine_Infection_Type
UNSET CellLine

