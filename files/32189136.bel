##################################################################################
# Statements #
##################################################################################

SET Citation = {"PubMed","32189136"}
SET PublicationType = "Letter to Editor"
SET PublicationStatus = "Published"
SET Section = "Full Text"
SET Custom_Disease = "COVID-19"
SET Species = "9606"

SET Evidence = "Once SpO2 < 93%, RR > 30/min, HR > 120/min or any signs of organ failure were observed, patients would be transferred to intensive care unit (ICU) and ICU physicians and nurses would take over their treatment."

SET Severity = "Severe"
bp(EFO:"oxygen saturation measurement") neg path(DO:"COVID-19")
bp(MESH:"Respiratory Rate") neg path(DO:"COVID-19")
bp(MESH:"Heart Rate") pos path(DO:"COVID-19")
path(MESH:"Multiple Organ Failure") pos path(DO:"COVID-19")

SET Evidence = "From our data of more than 600 NCP patients in Jiangsu Province, age, lymphocyte count, oxygen supplementation and aggressive pulmonary radiographic infiltrations are independent risk factors for NCP progressing to a critical condition."

a(MESH:"Age of Onset") pos path(DO:"COVID-19")
a(MESH:Lymphocytes) neg path(DO:"COVID-19")
path(HP:"Pulmonary infiltrates") reg path(DO:"COVID-19")
bp(MESH:"Oxygen Inhalation Therapy") -- path(DO:"COVID-19")

SET Evidence = "For patients with ARDS or pulmonary extensive effusion in CT scan, high-flow nasal cannula oxygen therapy (HFNC) or non-invasive mechanical ventilation (NIV) was used to maintain positive end expiratory pressure (PEEP) to prevent alveolar collapse even if some of these patients did not have refractory hypoxemia."

path(EFO:"acute respiratory distress syndrome") pos path(DO:"COVID-19")
bp(MESH:"Oxygen Inhalation Therapy") neg path(EFO:"acute respiratory distress syndrome")
bp(MESH:"Noninvasive Ventilation") neg path(EFO:"acute respiratory distress syndrome")
bp(MESH:"Noninvasive Ventilation") pos path(HP:"alveolar system")

SET Evidence = "(2) Restrictive fluid resuscitation under the premise of adequate tissue perfusion is performed to relieve pulmonary edema."

path(DO:"pulmonary edema") pos path(DO:"COVID-19")
bp(MESH:"Fluid Therapy") -| path(DO:"pulmonary edema")

SET Evidence = "Although previous study proved prone position’s benefit in moderate-to-severe ARDS patients with invasive mechanical ventilation (IMV) [11], we attempted awake prone position in NCP patients which showed significant effects in improving oxygenation and pulmonary heterogeneity (Fig. ​(Fig.2).2)."

bp(MESH:"Pulmonary Ventilation") -| path(EFO:"acute respiratory distress syndrome")
bp(MESH:"Pulmonary Ventilation") -> bp(EFO:"oxygen saturation measurement")
UNSET Severity
UNSET Species
UNSET Custom_Disease
UNSET PublicationType
UNSET PublicationStatus
UNSET Section
