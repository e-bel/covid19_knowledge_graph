##################################################################################
# Statements #
##################################################################################

SET Citation = {"PubMed","32193831"}
SET PublicationType = "Case Report"
SET PublicationStatus = "Preprint"
SET Section = "Results"
SET Custom_Disease = "COVID-19"
SET Species = "9606"
SET population_type = "Pediatric"

SET Evidence = "In the severe pediatric patients, the most common symptom was polypnea (8/8), followed by fever (6/8), cough (6/8), expectoration (4/8), nausea/vomiting (4/8), diarrhea (3/8), fatigue/myalgia (1/8), headache (1/8) and constipation (1/8) (Table 1)."

SET Severity = "Severe"
path(DO:"COVID-19") pos path(HP:"Breathing dysregulation")
path(DO:"COVID-19") pos path(MESH:Fever)
path(DO:"COVID-19") pos path(MESH:Cough)
path(DO:"COVID-19") pos path(MESH:Fatigue)
path(DO:"COVID-19") pos path(MESH:Myalgia)
path(DO:"COVID-19") pos path(MESH:Vomiting)
path(DO:"COVID-19") pos path(DO:diarrhea)
path(DO:"COVID-19") pos path(DO:constipation)
path(DO:"COVID-19") pos path(MESH:Headache)
path(DO:"COVID-19") pos a(MESH:Expectorants)

SET Evidence = "Six patients had bilateral pneumonia and two unilateral pneumonia."

path(DO:"COVID-19") pos path(DO:pneumonia)

SET Evidence = "Increased C-reactive protein, procalcitonin and lactate dehydrogenase were found in five patients (5/8), and increased alanine aminotransferase (ALT) in four (4/8). "

SET MeSHAnatomy = "Blood"
path(DO:"COVID-19") pos p(HGNC:GPT)
path(DO:"COVID-19") pos p(HGNC:LDHA)
path(DO:"COVID-19") pos p(HGNC:LDHB)
path(DO:"COVID-19") pos p(HGNC:LDHC)
path(DO:"COVID-19") pos p(HGNC:CRP)
path(DO:"COVID-19") pos p(MESH:preprocalcitonin)

SET Evidence = "Two patients (2/5) had high D-dimer level."

path(DO:"COVID-19") pos bp(EFO:"D dimer measurement")

SET Evidence = "TBNK lymphocyte subset analysis revealed decreased percentage of CD16 + CD56 + lymphocytes (4/8) and Th/Ts (1/8), and increased percentage of CD3 + (2/8), CD4 + (4/8) and CD8 + (1/8) lymphocytes. Cytokine detection assays showed increased levels of IL-6 (2/8), IL-10 (5/8) and IFN-γ (2/8)."

path(DO:"COVID-19") neg a(FIXME:"CD16 positive CD56 positive lymphocyte")
path(DO:"COVID-19") pos a(EFO:"CD4-positive helper T cell")
path(DO:"COVID-19") pos a(EFO:"CD8-Positive T-Lymphocytes")
path(DO:"COVID-19") neg a(FIXME:"CD3-positive lymphocyte")
path(DO:"COVID-19") pos p(HGNC:IL6)
path(DO:"COVID-19") pos p(HGNC:IL10)
path(DO:"COVID-19") pos p(HGNC:IFNG)
UNSET MeSHAnatomy

SET Evidence = "Among the eight patients, six (6/8) received high-flow oxygen therapy and two critically ill (2/8) patients received mechanical ventilation."

bp(MESH:"Oxygen Inhalation Therapy") -| path(DO:"COVID-19")
bp(MESH:Ventilation) -| path(DO:"COVID-19")

SET Evidence = "All patients received antiviral treatments (virazole, oseltamivir and interferon). "
a(MESH:"adenylyl-(3'-5')-virazole") -| path(DO:"COVID-19")
a(CHEBI:interferon) -| path(DO:"COVID-19")
a(CHEBI:oseltamivir) -| path(DO:"COVID-19")

SET Evidence = "Antibiotics, traditional Chinese medicine, intravenous glucocorticoids and immunoglobulin were also used dependent on patients’ conditions."

a(MESH:"Medicine, Chinese Traditional") -| path(DO:"COVID-19")
bp(GOBP:"response to antibiotic") -| path(DO:"COVID-19")
a(CHEBI:glucocorticoid) -| path(DO:"COVID-19")
UNSET Severity
UNSET population_type
UNSET Species
UNSET Custom_Disease
UNSET PublicationType
UNSET PublicationStatus
UNSET Section