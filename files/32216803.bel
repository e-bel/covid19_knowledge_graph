##################################################################################
# Statements #
##################################################################################

SET Citation = {"PubMed", "Respir Res. 2020 Mar 26;21(1):74. doi: 10.1186/s12931-020-01338-8", "32216803"}
SET PublicationType = "Case Report"
SET Section = "Results"
SET Custom_Disease = "COVID-19"
SET Species = "9606"

SET Evidence = "As shown by Table 1, <40 years or >60 years group was related to \
higher risk of reaching the composite endpoint (R2 = 0.203, \
P4 = 0.049)."

SET Cohort_Age ="below 40"
a(MESH:"Age of Onset") neg path(MESH:Mortality)
#Composite endpoint is coded as mortality
SET Cohort_Age = "above 60"
a(MESH:"Age of Onset") pos path(MESH:Mortality)
UNSET Cohort_Age

SET Evidence = "Higher temperature was related to severe 2019 novel coronavirus \
pneumonia and composite endpoint. (R1 = 0.362, R2 = 0.383, P3<0.001, \
P4<0.001)."

SET Severity = "Severe"
bp(MESH:"Body Temperature") pos path(MESH:Mortality)

SET Evidence = "> 10*10^9/L group were strongly related to severe 2019 novel \
coronavirus pneumonia and composite endpoint. (R1 = 0.604, P3<0.001, \
R2 = 0.543, P4<0.001)."

SET MeSHAnatomy = "Blood"
a(MESH:Leukocytes) pos path(MESH:Mortality)

SET Evidence = "Higher neutrophil percentage was strongly related to severe 2019 novel \
coronavirus pneumonia and composite endpoint. (R1 = 0.641, P3<0.001, \
R2 = 0.492, P4<0.001)."

a(MESH:Neutrophils) pos path(MESH:Mortality)

SET Evidence = "Lower lymphocyte count was strongly related to severe 2019 novel \
coronavirus pneumonia and composite endpoint. (R1 = -0.451, P3<0.001, \
R2 = -0.415, P4<0.001)."

a(MESH:Lymphocytes) neg path(MESH:Mortality)

SET Evidence = "Higher C-reactive protein level was strongly related to severe 2019 \
novel coronavirus pneumonia and composite endpoint. (R1 = 0.572, \
P3<0.001, R2 = 0.503, P4<0.001)."

a(MESH:"C-Reactive Protein") pos path(MESH:Mortality)

SET Evidence = "Higher D-dimer level was strongly related to severe 2019 novel \
coronavirus pneumonia and composite endpoint. (R1 = 0.717, \
P3<0.001,R2 = 0.737,P4<0.001)."

bp(EFO:"D dimer measurement") pos path(MESH:Mortality)

SET Evidence = "Higher alanine aminotransferase activity was related to severe 2019 \
novel coronavirus pneumonia and composite endpoint. (R1 = 0.243, \
P3 = 0.013, R2 = 0.306, P4 = 0.002)."

act(p(HGNC:GPT)) pos path(MESH:Mortality)

SET Evidence = "Higher aspartate aminotransferase activity was related to severe 2019 \
novel coronavirus pneumonia and composite endpoint. (R1 = 0.202, \
P3 = 0.042, R2 = 0.325, P4 = 0.001)."

act(p(HGNC:GOT1)) pos path(MESH:Mortality)
act(p(HGNC:GOT2)) pos path(MESH:Mortality)

SET Evidence = "Higher α - hydroxybutyrate dehydrogenase activity correlated strongly \
with severe 2019 novel coronavirus pneumonia and composite endpoint. \
(R1 = 0.449, P3<0.001, R2 = 0.439, P4<0.001)."

act(p(HGNC:BDH1)) pos path(MESH:Mortality)

SET Evidence = "Higher lactate dehydrogenase activity correlated strongly with severe \
2019 novel coronavirus pneumonia and composite endpoint. (R1 = 0.454, \
P3<0.001, R2 = 0.453, P4<0.001)."

act(p(HGNC:LDHA)) pos path(MESH:Mortality)
act(p(HGNC:LDHB)) pos path(MESH:Mortality)

SET Evidence = "Higher creatine kinase activity correlated strongly with severe 2019 \
novel coronavirus pneumonia and composite endpoint. (R1 = 0.231, \
P3 = 0.019, R2 = 0.289, P4 = 0.003)."

act(p(HGNC:CKB)) pos path(MESH:Mortality)

SET Evidence = "Lower total protein level correlated with severe 2019 novel \
coronavirus pneumonia and composite endpoint. (R1 = − 0.340, \
P3 = 0.001, R2 = − 0.252, P4 = 0.015)."

a(MESH:"Blood Proteins") neg path(MESH:Mortality)
#a used instead of p , as mentioned in MESH file. 
UNSET Severity
UNSET MeSHAnatomy
UNSET Species
UNSET Custom_Disease
