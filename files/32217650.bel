# Statements #
##################################################################################

SET Citation = {"PubMed","32217650"}
SET PublicationType = "Case Report"
SET PublicationStatus = "Published"
SET Section = "Results"
SET Custom_Disease = "COVID-19"
SET Species = "9606"

SET Evidence = "The most common symptom was fever on or after hospitalisation (88.0%), \
followed by dry cough (70.2%). Fatigue (42.8%) and productive cough \
(36.0%) were less common."

path(DO:"COVID-19") pos path(MESH:Fever)
path(DO:"COVID-19") pos path(MESH:Cough)
path(DO:"COVID-19") pos path(MESH:Fatigue)

SET Evidence = "At least one abnormal chest CT manifestation (including ground-glass \
opacities, pulmonary infiltrates and interstitial disorders) was \
identified in more than 70% of patients."

SET Technique = "computed tomography"
path(DO:"COVID-19") pos path(DO:"interstitial lung disease")
path(DO:"COVID-19") pos path(HP:"Pulmonary infiltrates")
UNSET Technique

SET Evidence = "The prevalence of specific comorbidities was: hypertension (269; \
16.9%), other cardiovascular diseases (53.7%) cerebrovascular diseases \
(30; 1.9%), diabetes (130; 8.2%), hepatitis B infections (28; 1.8%), \
chronic obstructive pulmonary disease (24; 1.5%), chronic kidney \
diseases (21; 1.3%), malignancy (18; 1.1%) and immunodeficiency (3; \
0.2%)."

path(DO:"COVID-19") pos path(DO:hypertension)
path(DO:"COVID-19") pos path(DO:"cardiovascular system disease")
path(DO:"COVID-19") pos path(DO:"cerebrovascular disease")
path(DO:"COVID-19") pos path(DO:"diabetes mellitus")
path(DO:"COVID-19") pos path(DO:"hepatitis B")
path(DO:"COVID-19") pos path(DO:"chronic obstructive pulmonary disease")
path(DO:"COVID-19") pos path(DO:"chronic kidney disease")
path(DO:"COVID-19") pos path(DO:"cancer")
path(DO:"COVID-19") pos path(DO:"common variable immunodeficiency")

SET Evidence = "Patients with at least one comorbidity were older (mean: 60.8 versus \
44.8 years), were more likely to have shortness of breath (41.4% \
versus 17.8%), nausea or vomiting (10.4% versus 4.3%), and tended to \
have abnormal chest X-ray manifestations (29.2% versus 15.1%) (table \
1)."

path(DO:"COVID-19") pos path(HP:"Breathing dysregulation")
path(DO:"COVID-19") pos path(MESH:Vomiting)
UNSET Custom_Disease
UNSET Species
UNSET PublicationType
UNSET PublicationStatus
UNSET Section
